{
  "id": "AB440SC",
  "name": {
    "en": "AB440SC"
  },
  "$extends": ["defaults", "rf.dipswitch_socket"],
  "rf": {
      "signal": "elro_dipswitch"
  },
  "$pairOptions": {
      "rf.choice": {
          "choices": [
            {
              "name": "rf.pair.choice.buttons.copy.default",
              "view": "rf.imitate",
              "svg": "assets/remote.svg"
            },
            {
              "name": "rf.pair.choice.buttons.generate.dipswitch",
              "view": "rf.info",
              "svg": "assets/dipswitches.svg"
            }
          ]
      },
      "rf.imitate": {
          "svg": "assets/remote.svg"
      },
      "rf.info": {
          "svg": "assets/dipswitches.svg"
      },
      "rf.dipswitch": {
          "dipswitchList": [
              1, 2, 3, 4, 5,
              "A", "B", "C", "D", "E"
          ]
      }
  }
}
